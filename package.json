  ],
  "author": "Aliaksandr Pyrkh <alexpyrkh@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/apyrkh/up.js/issues"
  },
  "homepage": "https://github.com/apyrkh/up.js#readme",
  "dependencies": {},
  "devDependencies": {
    "google-closure-compiler": "20200112.0.0",
    "husky": "4.2.1"
  }
}
